{
  "name": "site-pe",
  "version": "1.0.0",
  "description": "Site d'entraînement à l'examen du PE pour les scouts du groupe NDC",
  "main": "build.js",
  "scripts": {
    "build": "node build.js",
    "start": "npm run build && http-server docs -p 8000 -o",
    "test": "node --test tests",
    "generate:series": "node scripts/generate-exam-series.js",
    "generate:qcm:large": "node scripts/generate-large-qcm.js --variants=150 --objectiveVariants=3 --seed=20260212",
    "import:attendus": "node scripts/import-attendus-json.js",
    "generate:pe:data": "node scripts/generate-pe-datasets.js --perFact=8 --seed=20260212"
  },
  "keywords": ["pe", "patron", "embarcation", "scout", "navigation"],
  "author": "Philibert Pap",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^11.2.0",
    "mustache": "^4.2.0",
    "http-server": "^14.1.1"
  }
}
